/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.m-b-10 {
  margin-bottom: 10rpx;
}
.m-l-10 {
  margin-left: 10px;
}
.btn-primary {
  color: #fff !important;
  background-color: #0492CE !important;
  border: 1px solid #0492CE !important;
  display: flex;
}
.btn-primary-ghost {
  color: #0492CE !important;
  background-color: transparent !important;
  border: 1px solid #0492CE !important;
}
.m-b-20 {
  margin-bottom: 20rpx;
}
.m-t-20 {
  margin-top: 20rpx;
}
.m-t-30 {
  margin-top: 30px;
}
.m-b-30 {
  margin-bottom: 30rpx;
}
.m-b-40 {
  margin-bottom: 40rpx;
}

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.ellipsis-multi-2 {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.ellipsis-multi-3 {
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  overflow: hidden;
}
.item-icon {
  width: 40rpx;
  height: 40rpx;
}
.my-btn {
  width: auto;
  padding: 0;
  font-weight: 400;
  border-radius: 0;
}
.my-btn.button-hover,
.navigator-hover {
  background-color: rgba(128, 128, 128, 0.1);
  opacity: 0.7;
}
.center {
  text-align: center;
}
page {
  background: #F6F6F6;
}

